<?xml version='1.0' encoding='utf-8'?>
<widget id="em.demos.dice" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="windows-target-version" value="8.0" />
    <name>Dice</name>
    <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">EricMitt</author>
    <content src="index.html" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <vs:plugin name="org.apache.cordova.device-motion" version="0.2.12-dev" />
    <vs:platformSpecificValues />
</widget>
